pipeline{
parameters {
        string(name: 'VERSION', choices: ['1.1.0', '1.2.0', '1.3.0'], description: '')
    }

agent any

stages {
       stage ("build") {
             steps {
                      echo 'building'
                    
           }
        stage ("test") {
              steps {
                      echo 'testing'
                     }
                         }
       }
}
